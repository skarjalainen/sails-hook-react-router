{
  "name": "sails-hook-react-router",
  "version": "0.1.1",
  "description": "React universal router for sails.",
  "keywords": [
    "sails",
    "sailsjs",
    "hook",
    "sails-hook",
    "webpack",
    "react",
    "isomorphic",
    "router",
    "universal",
    "react-router",
    "build",
    "asset pipeline"
  ],
  "dependencies": {
    "sails": "^0.12.3"
  },
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-syntax-export-extensions": "^6.5.0",
    "babel-plugin-transform-export-extensions": "^6.5.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^2.7.0",
    "eslint-config-airbnb": "^7.0.0",
    "eslint-plugin-jsx-a11y": "^0.6.2",
    "eslint-plugin-react": "^4.3.0",
    "isomorphic-style-loader": "0.0.12",
    "react": "^15.0.1",
    "react-router": "^2.1.1"
  },
  "scripts": {
    "prepublish": "npm run lint && npm run build",
    "lint": "eslint src/",
    "build": "NODE_ENV=production babel src --out-dir lib --copy-files",
    "build-watch": "NODE_ENV=production babel src --out-dir lib --copy-files --watch src",
    "test": "mocha"
  },
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/teamfa/sails-hook-react-router.git"
  },
  "author": {
    "name": "Mike Diarmid",
    "email": "mike.diarmid@teamfa.com"
  },
  "contributors": [
    "Elliot Hesp"
  ],
  "sails": {
    "isHook": true,
    "hookName": "react"
  },
  "license": "MIT"
}
